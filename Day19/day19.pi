import util.
import cp.
import testing.

makeable_towels(Input) = Num, Input.split().length <= 3 => 
    Num = 1.

makeable_towels(Input) = Num, Input.split().length > 3 => 
    Num = 2.

all_tests = [

    testing.assert_equal(        
        "simple_case",
        makeable_towels("r

r"), 1),

    testing.assert_equal(
        "two_patterns_two_combos_no_mixing",
        makeable_towels("r, wq

r
wq
q"), 2)
].

main =>  
    Tests = all_tests(),
    testing.test_report(Tests)=Tests.length.
